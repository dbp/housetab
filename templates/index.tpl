<apply template="base">

  <h1>HouseTab [<loggedInAccount/>]</h1>

  <table>
    <tr>
      <th>Date</th>
      <th>Description</th>
      <th>By</th>
      <th>For</th>
      <th>Amount</th>
    </tr>
    <entries>
      <tr>
        <td>
          <date/>
        </td>
        <td>
          <description/>
        </td>
        <td>
          <who-id/>
        </td>
        <td>
          <whopays>
            <id/>
          </whopays>
        </td>
        <td>
          <howmuch/>
        </td>
      </tr>
    </entries>
  </table>

</apply>
